  // END SECTION :: review

  section
    .review
      .container
        h2(data-aos='fade-up') Отзывы франчайзи
        .review__carousel(js-review-slider data-aos='fade-up')
          .review__item
            .review__item-image
              a.no-barba(href="img/review.jpg" js-open-photo data-effect="mfp-zoom-in")
                img(src="img/review.jpg", alt="")
            p г.Львов
