  section
    .anketa
      .container
        h2 Хотите быть нашим франчайзи ? Заполните анкету !
        form.form-anketa(action="#" method="post")
          .anketa__grid
            .input-container
              input(type="text" name="name" placeholder="Ваше имя")
            .input-container
              input(type="text" name="surname" placeholder="Ваша фамилия")
